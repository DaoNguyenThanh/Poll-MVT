extends ../layout

block content
    #container.container
        .form-container.sign-up-container
            form.frm-sign(action='#')
                h1.title-sign Create Account
                .social-container
                    a.social(href='#')
                        i.fab.fa-facebook-f
                    a.social(href='#')
                        i.fab.fa-google-plus-g
                    a.social(href='#')
                        i.fab.fa-linkedin-in
                span or use your email for registration
                input.ip(type='email' placeholder='Email' required='')
                input.ip(type='password' placeholder='Password' required='')
                button.btn-sign Sign Up
        .form-container.sign-in-container
            form.frm-sign(action='#')
                h1.title-sign Sign in
                .social-container
                    a.social(href='#')
                        i.fab.fa-facebook-f
                    a.social(href='#')
                        i.fab.fa-google-plus-g
                    a.social(href='#')
                        i.fab.fa-linkedin-in
                span or use your account
                input.ip(type='email' placeholder='Email' required='')
                input.ip(type='password' placeholder='Password' required='')
                a(href='#') Forgot your password?
                button.btn-sign Sign In
        .overlay-container
            .overlay
                .overlay-panel.overlay-left
                    h1.title-sign Welcome Back!
                    img.logo-img(src="data:image/jpeg;base64,", alt="")
                    p To keep connected with us please login with your personal info
                    button.btn-sign#signIn.ghost Sign In
                .overlay-panel.overlay-right
                    h1.title-sign Hello, Friend!
                    img.logo-img(src="data:image/jpeg;base64,", alt="")
                    p Enter your personal details and start journey with us
                    button.btn-sign#signUp.ghost Sign Up

